<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="/stylesheets/other.css">
    <link href="https://fonts.googleapis.com/earlyaccess/hannari.css" rel="stylesheet">
    <title>ボード</title>
  </head>
  <body class="container">
     <%- include('../header'); %>
      <div role="main">
        </div>
          <div class="management_container">
            <table border="" bordercolor="#EEEEEE" class="management_table">
                <tr>
                    <td colspan="5" class="boke"><p><%= username %> さんだよね</p></td>
                </tr>
                <tr>
                    <th bgcolor="">ID</th>
                    <th bgcolor="">ユーザーID</th>
                    <th bgcolor="">Message</th>
                    <th bgcolor="">編集</th>
                    <th bgcolor="">削除</th>
                </tr>
                    <% for(var i in content){ %>
                    <tr>
                        <% var obj = content[i]; %>
                        <td><%= obj.id %></td>
                        <td><%= obj.userId %></td>
                        <td><%= obj.message %></td>
                        <td>
                            <a href="#">くわしく</a>
                        </td>
                        <td><form action="#" method="POST">
                                <input type="hidden" name="id" value="#">
                                <button type="submit" value="削除" class="btn2">✔</button>
                            </form>
                        </td>
                    </tr>
                        <% } %>
            </table>
        </div>
    </div>
    <div class="neko">
      <a href="../../add" class="btn-a">もふもふの猫さん</a>
  </div>
    <%- include('../footer'); %>
  </body>
</html>